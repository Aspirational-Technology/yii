  "bin": [
    "framework/yiic"
  ],
  "archive": {
    "exclude": [
      "demos"
    ]
  },
  "require": {
    "php": ">=8.1.0",
    "ext-mcrypt": "*",
    "ext-openssl": "*",
    "ext-memcached": "*"
  },
  "autoload": {
    "classmap": [
      "framework/YiiBase.php",
      "framework/yii.php"
    ]
  },
  "require-dev": {
    "cweagans/composer-patches": "^1.7",
    "phpunit/phpunit": "4.8.34",
    "phpunit/phpunit-selenium": "~1.4.0",
    "phing/phing": "2.*",
    "phpseclib/mcrypt_compat": "^1.0",
    "pear/archive_tar": "~1.4.6",
    "symfony/flex": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "symfony/flex": true
    },
    "platform": {
      "ext-mcrypt": "5.6.40",
      "ext-memcached": "8.1.16",
      "ext-openssl": "8.1.16"
    }
  }
